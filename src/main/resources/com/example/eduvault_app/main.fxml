<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.eduvault_app.Main"
            prefHeight="400.0" prefWidth="600.0">

</AnchorPane>

<BorderPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.project.controller.MainController">
<top>
    <MenuBar>
        <Menu text="File">
            <MenuItem text="Exit" onAction="#handleExit"/>
        </Menu>
        <Menu text="View">
            <MenuItem text="Users" onAction="#showUsers"/>
            <MenuItem text="Notifications" onAction="#showNotifications"/>
        </Menu>
    </MenuBar>
</top>
<center fx:id="contentPane"/>
</BorderPane>

